{
  "name": "@cuperman/cdk-appconfig",
  "description": "High-level CDK Constructs for AWS AppConfig",
  "license": "MIT",
  "version": "1.0.0",
  "author": {
    "name": "Jeff Cooper",
    "email": "jeff@cuperman.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cuperman/cdk-appconfig",
    "path": "packages/cdk-appconfig"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "jsii",
    "watch": "jsii --watch",
    "package": "jsii-pacmak",
    "test": "jest",
    "prepare": "yarn clean && yarn build",
    "prepublishOnly": "yarn test && yarn package"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.0.0",
    "aws-cdk-lib": "2.38.1",
    "constructs": "10.1.84",
    "jest": "^26.4.2",
    "jsii": "^1.27.0",
    "jsii-pacmak": "^1.27.0",
    "ts-jest": "^26.2.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@cuperman/cdk-appconfig-handler-hosted-configuration-version": "^1.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.38.1",
    "constructs": "^10.1.84"
  },
  "bundledDependencies": [
    "@cuperman/cdk-appconfig-handler-hosted-configuration-version"
  ],
  "stability": "experimental",
  "jsii": {
    "tsc": {
      "outDir": "dist"
    },
    "outdir": "pkg",
    "versionFormat": "full",
    "targets": {
      "python": {
        "distName": "cdk-appconfig",
        "module": "cdk_appconfig"
      }
    }
  }
}
