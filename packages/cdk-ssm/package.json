{
  "name": "@cuperman/cdk-ssm",
  "description": "High-level CDK Constructs for AWS SSM",
  "license": "MIT",
  "version": "0.7.0",
  "author": {
    "name": "Jeff Cooper",
    "email": "jeff@cuperman.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cuperman/cdk-appconfig",
    "path": "packages/cdk-ssm"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "jsii",
    "watch": "jsii --watch",
    "package": "jsii-pacmak",
    "test": "jest",
    "prepare": "run-s clean build",
    "prepublishOnly": "run-s test package"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.0.0",
    "@types/jest": "^26.0.10",
    "@types/node": "14.17.0",
    "aws-cdk": "^1.0.0",
    "jest": "^26.4.2",
    "jsii": "^1.27.0",
    "jsii-pacmak": "^1.27.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.2.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-ssm": "^1.0.0",
    "@aws-cdk/core": "^1.0.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-ssm": "^1.0.0",
    "@aws-cdk/core": "^1.0.0",
    "constructs": "^3.0.0"
  },
  "stability": "experimental",
  "jsii": {
    "tsc": {
      "outDir": "dist"
    },
    "outdir": "pkg",
    "versionFormat": "full",
    "targets": {
      "python": {
        "distName": "cdk-ssm",
        "module": "cdk_ssm"
      }
    }
  }
}
